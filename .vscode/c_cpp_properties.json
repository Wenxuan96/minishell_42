{
    "configurations": [
      {
        "name": "Linux",
        "includePath": [
          "${workspaceFolder}/include",
          "${workspaceFolder}/libft",
          "${workspaceFolder}/src",
          "${workspaceFolder}/src/builtins",
          "/usr/include",
          "/usr/include/x86_64-linux-gnu",  // adjust if needed
          "/usr/local/include"
        ],
        "defines": [],
        "compilerPath": "/usr/bin/gcc",
        "cStandard": "c11",
        "cppStandard": "c++17",
        "intelliSenseMode": "linux-gcc-x64"
      }
    ],
    "version": 4
}
  
