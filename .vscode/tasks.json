{
	"version": "2.0.0",
	"tasks": [
	  {
		"label": "Build Minishell",
		"type": "shell",
		"command": "make",
		"options": {
		  "cwd": "${workspaceFolder}"
		},
		"group": {
		  "kind": "build",
		  "isDefault": true
		},
		"problemMatcher": [
		  "$gcc"
		]
	  }
	]
  }


